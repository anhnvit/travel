* {
    image-rendering: optimizequality;
    }
.tourDetail .ant-pro-page-container-children-content{
    margin-top: 0px !important;
}
.tourDetail .panel-time {
    font-size: 16px;
}
.tourDetail .note {
   /* font-weight: 500; */
    margin-left: 24px;
    line-height: 24px;
}
.tourDetail .note::before {
    content: '';
    display: inline-block;
   height: 14px;
   width: 14px;
   border-radius: 14px;
   -moz-border-radius: 14px;
   -webkit-border-radius: 14px;
   position: relative;
   top: 2px;
   margin-right: 5px;
}
.tourDetail .note.green::before{
    background:#52c41a;
}
.tourDetail .note.red::before{
    background:#ff4d4f;
}
.tourDetail .note.yellow::before{
    background:#faad14;
}
.tourDetail .box-weather {
    border-radius: 2px;
    min-height: 100px;
    display: inline-flex;
    background: #f9f9f9;
    width: calc(100% - 20px);
    /* margin: 20px auto; */
    border: solid 2px #f9f9f9;
    position: relative;
    transform: translateY(25%);
}
.tourDetail .box-weather .text-weather {
    padding: 16px;
}
.tourDetail .card-item .ant-card-body{
    padding: 0px;
}
.tourDetail .card-img{
    width: 28px;
    height: 28px;
    margin-top: -5px;
    margin-bottom: 2px;
}
.tourDetail .box-weather .box-img{
    margin: 16px 0 16px 10px;
    width: auto;
    height: 90px;
}
.tourDetail .box-card .ant-space{
    vertical-align: middle;
}
.tourDetail .space-align-container {
    display: flex;
    align-item: flex-start;
}
.tourDetail .space-align-block .mock-block {
    padding: 32px 8px 16px;
    background: rgba(150, 150, 150, 0.2);
}
.tourDetail .timeline{
    padding: 12px 20px;
    height: 84px;
}
.tourDetail .fixed {
    position: sticky;
    left: 0;
    background: #f0f2f5;
    z-index: 2;

}
.tourDetail .box-time {
    margin: 0px;
    height: 84px;
    width: 26px;
    position: relative;
    margin-bottom: 0px !important

  }
.tourDetail .box-card{
    height: 84px;
    border-bottom: 1px dashed #bac3cf;
}
.tourDetail .box-card .ant-card-head-title {
    padding: 3px 0px !important;
    font-size: 12px;
    text-align: center;
  }
.tourDetail .box-card .ant-card-head{
    min-height: 26px;
    border: none;
  }
.tourDetail .box-time-none {
    height:50px;
    width:26px;
    display: block;
    position: relative;
    background-color: rgb(0, 21, 41);
  }
.tourDetail .text-time {
    text-transform: uppercase;
    color: white;
    transform-origin: 0 0;
    transform: rotate(-90deg);
    text-align: center;
}
.tourDetail .tour-header-code {
    font-size: 20px;
    color: #1890ff;
    padding-right: 16px;
    font-weight: 600;
    text-transform: uppercase;
    display: inline-flex;
}
.tourDetail .tour-header-name {
    font-size: 20px;
    font-weight: 600;
    /* padding-right: 10px; */
    display: inline;
}
.tourDetail .ant-pro-page-container-main .ant-pro-page-container-extraContent{
    margin-left: 0;
    width: 100%;
    text-align: left;
}
/* .tourDetail .tour-header{
    margin-bottom: 16px;
} */
.tourDetail .col-weather{
    width: 24px;
    margin-right: 16px;
    background: #d9e0e8;
}
.tourDetail .form-share .ant-form-item-control-input-content {
    display: inline-flex !important;
}
.tourDetail .ant-collapse{
    border: none;
}
.tourDetail .ant-collapse > .ant-collapse-item{
    border-bottom: none;
}
.tourDetail .justify-content-end{
    display: flex;
    /* justify-content: flex-end; */
}
.tourDetail .ant-space-item{
    margin-bottom: 0!important;
}
.tourDetail .cardItem .ant-card{
    box-shadow: 0 2px 2px 0 rgba(0, 0, 0, 0.15);
}
.tourDetail .cardItem .ant-btn{
    width: calc(100% - 40px) !important;
    height: 28px;
    line-height: 0.5;
    box-shadow: none;
    margin: 10px 20px;
}
.tourDetail .cardItem .ant-card-bordered{
    border: none;
}

.tourDetail .cardItem .ant-btn-dashed{
    border-color: #4e5a6b;
    background-color: transparent;
    color: #4e5a6b;
}

.tourDetail .cardItem .ant-btn-default{
    border-color: #D9E0E8;
    background-color: #D9E0E8;
    color:#4E5A6B;
}
.tourDetail .cardItem .ant-space-item:nth-child(3) > .box-card{
    border: none;
}
.tourDetail .cardItem .ant-card-body .tempMax{
    font-size: 21px;
}
.tourDetail .cardItem .ant-card-body .tempMin{
    font-size: 17px;
    padding-left: 5px;
    opacity: 0.6;
}
.tourDetail sup{
    font-size: 10px;
    position: relative;
    top: -10px
}
.tourDetail .panel-time .card-img{
    width: 60px;
    height: 60px;
    margin-left: 12px;
}

.tourDetail .ant-timeline-item-head{
    width: 18px;
    height: 18px;
    border: solid 3px #d9e0e8;
    background: #959EAB;
    margin-top: 40px;
}
.tourDetail .ant-timeline-item-content > .element{
    padding-top: 19px;
}
.tourDetail .ant-timeline-item-content{
    padding-top: 9px;
}
.tourDetail .panel-time .location{
    line-height: 1.5;
    max-height: 50px;
    overflow: hidden;
    margin: auto 0;
}
.tourDetail .ant-timeline-item-tail{
    left: 8px;
    border-left: 3px solid #f0f2f5;
    top: 50px;
}
.tourDetail .ant-collapse-icon-position-right > .ant-collapse-item.panel-time  > .ant-collapse-header{
    padding: 0;
    background-color:#f0f2f5;
}

.tourDetail .ant-collapse-icon-position-right > .ant-collapse-item-disabled.panel-time  > .ant-collapse-header{
    padding: 0;
    background-color:#f0f2f5;
    color: rgba(0, 0, 0, 0.85);
}
.tourDetail .ant-collapse-icon-position-right > .ant-collapse-item-disabled.panel-time  > .ant-collapse-header img,
.tourDetail .ant-collapse-icon-position-right > .ant-collapse-item-disabled.panel-time  > .ant-collapse-header span{
    display: none;
}

.tourDetail .panel-time .ant-collapse-header .ant-row{
    min-height: 68px;
    line-height: 68px;
}
.tourDetail .panel-time .ant-collapse-content{
    border: none;
}
.tourDetail .panel-time .tempMax ,.panel-time .tempMin{
    font-size: 24px;
}
.tourDetail .panel-time .tempMax{
    padding-left: 10px;
    padding-right: 5px;
}
.tourDetail .panel-time .tempMin{
    opacity: 0.5;
}
.tourDetail .ant-collapse-icon-position-right > .ant-collapse-item > .ant-collapse-header .ant-collapse-arrow {
    right: 24px;
    left: auto;
    top: 20px;
    border: solid 1px #000000;
    border-radius: 100px;
    padding: 3px;
    opacity: 0.4;
}
.tourDetail .ant-collapse .ant-collapse-item-disabled > .ant-collapse-header > .anticon{
    display: none;
}
.tourDetail .panel-time .ant-collapse-content > .ant-collapse-content-box{
    padding: 0
}
.tourDetail .panel-time .weather-index img{
    margin-right: 10px;
    margin-left: 16px;
}
.tourDetail .panel-time .weather-index{
    column-count: 2;
    margin: 16px 0;
}
.tourDetail .panel-time .weather-index p:first-child{
    margin-top: auto;
}
.tourDetail .panel-time .weather-index p{
    color:#00000087;
    margin: 34px 0 0 0;
}
.tourDetail .ant-collapse-icon-position-right > .ant-collapse-item.panel-time > .ant-collapse-header[aria-expanded="true"]{
    background-color: #f9f9f9;
}
.sc-ifAKCX{
    display: none;
}

.tourDetail .upcomming .ant-timeline-item-head {
    background: #ffffff;
}
.tourDetail .active .ant-timeline-item-head {
    background:#1890ff;
}
.tourDetail .tour-header-expand{
    color: #000000a3;
    /* margin-bottom: 16px; */
}
.tourDetail .tour-header-expand strong {
    color: #000000;
    font-weight: 600;
    padding-left: 5px;
}
.tourDetail .tour-header-expand div{
    display: inline-flex;
    margin-top: 5px;
}
.tourDetail .ant-page-header-heading-left{
    display: none;
}
.tourDetail .ant-page-header-heading-extra{
    margin: 0;
    width: 100%;
}

.tourDetail .sticky{
    position: fixed;
    top: 48px;
    width: calc(100% - 48px);
    z-index: 18;
    background: #ffffff;
}
.tourDetail .sticky.uncollapsed{
  width: calc(100% - 208px) !important;
}

.tourDetail .sticky + .ant-pro-grid-content{
    padding-top: 94px;
}
.tourDetail .title-detail{
    width: 30%;
}
.tourDetail .note-detail{
    width: 70%;
}
.tourDetail .note-detail .note-col{
    display: -webkit-inline-box;
    float: right
}
.tourDetail .ant-timeline-item{
    padding-bottom: 0;
}
.tourDetail .detail-left{
    width: 75%;
}
.tourDetail .detail-right{
    width: 25%;
}
.form-share .ant-form-item-control-input-content{
    display: flex !important;
}
.share-detail .ant-pro-page-container-warp{
    display: none
}
.share-detail .tour-header{
    padding: 16px 0
}
.tourDetail .scroll-container .space-align-container .cardItem{
    position: relative;
    left: 0;
}
@media (max-width: 1024px){
    .tourDetail .sticky, 
    .tourDetail .note-detail,
    .tourDetail .note-detail .note-col,
    .tourDetail .detail-left .tour-header-expand div,
    .tourDetail .detail-left .tour-header-expand span{
        width: 100%;
    }
    .tourDetail .title-detail{
        display: none;
    }
    .tourDetail .note-detail .note-col{
        display: flex;
    }
    .tourDetail .sticky{
        position: relative;
    }
    .tourDetail .form-share .ant-form-item-control-input-content{
        display: contents;
    }
    .tourDetail .row-note{
        padding: 12px 0 !important;
    }
    .mobile-detail .ant-modal-body{
        padding: 8px 5px 10px !important;
    }
    .mobile-detail .ant-modal-content,
    .mobile-detail .card-detail,
    .mobile-detail .header-modal{
        border-radius: 5px;
        -webkit-box-shadow: 4px 4px 8px rgba(0,30,84,.12),0 1px 3px rgba(0,30,84,.08)!important;
        box-shadow: 0 2px 2px 0 rgba(0, 0, 0, 0.15);
        border: .5px solid rgba(0,0,0,.1);
    }
    .mobile-detail .card-detail{
        margin: 10px 0 0;
    }
    .mobile-detail .header-modal{
        text-align: center;
        padding: 5px 0;
    }
    .share-detail .tour-header{
        padding: 16px 0 0 0;
    }


}
@media (max-width: 576px){
    .tourDetail .note {
        display: block;
    }
    .tourDetail .detail-right{
        display: grid;
     }
    /* .tourDetail .note-detail .note-col{
        display: flex;
        justify-content: flex-end;
    } */
}
@media (max-width: 1440px){
    .tourDetail .detail-left{
        width: 100%;
    }
    .tourDetail .detail-right{
        width: 100%;
        margin-top: 10px;
    }
    .tourDetail .detail-left .tour-header-expand div,
    .tourDetail .detail-left .tour-header-expand span{
        display: inline-block;
    }
    .tourDetail .tour-header-expand{
        margin: 15px 0;
        line-height: 2;
    }
}
