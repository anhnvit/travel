    "use-merge-value": "^1.0.1",
    "xlsx": "^0.16.8"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "^1.0.23",
    "@types/classnames": "^2.2.11",
    "@types/express": "^4.17.9",
    "@types/history": "^4.7.8",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.164",
    "@types/qs": "^6.9.5",
    "@types/react": "^16.9.55",
    "@types/react-dom": "^16.9.9",
    "@types/react-helmet": "^5.0.13",
    "@umijs/fabric": "^2.3.1",
    "@umijs/plugin-blocks": "^2.0.5",
    "@umijs/preset-ant-design-pro": "^1.3.0",
    "@umijs/preset-react": "^1.7.0",
    "@umijs/preset-ui": "^2.2.8",
    "axios": "^0.19.2",
    "carlo": "^0.9.46",
    "chalk": "^4.0.0",
    "cross-env": "^7.0.0",
    "cross-port-killer": "^1.1.1",
    "detect-installer": "^1.0.1",
    "enzyme": "^3.11.0",
    "eslint": "^7.12.1",
    "express": "^4.17.1",
    "gh-pages": "^3.0.0",
    "husky": "^4.3.0",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^10.5.1",
    "mockjs": "^1.0.1-beta3",
    "prettier": "^2.1.2",
    "pro-download": "1.0.1",
    "puppeteer-core": "^4.0.1",
    "query-string": "^6.13.6",
    "stylelint": "^13.8.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "checkFiles": [
    "src/**/*.js*",
    "src/**/*.ts*",
    "src/**/*.less",
    "config/**/*.js*",
    "scripts/**/*.js"
  ],
  "__npminstall_done": false
}
