    "bizcharts-plugin-slider": "^2.1.1-beta.1",
    "body-parser": "^1.19.0",
    "classnames": "^2.2.6",
    "cors": "^2.8.5",
    "data-grid": "^3.0.0",
    "devextreme": "20.2.3",
    "devextreme-react": "20.2.3",
    "dotenv": "^8.2.0",
    "dva": "^2.4.0",
    "excel4node": "^1.7.2",
    "exceljs": "^4.2.0",
    "express-fileupload": "^1.2.0",
    "file-saver": "^2.0.2",
    "gg-editor": "^2.0.2",
